pipeline {
    agent any

    stages {
        stage('Call another Jenkinsfile') {
            steps {
                script {
                    def params = [
                        string(branch: 'main', value: 'value1'),
                    ]

                    def jenkinsfile = sh(script: "curl -s https://github.com/laurabecerra1502/devops./jenkinsfile", returnStdout: true)
                    jenkinsfile = new GroovyShell().parse(jenkinsfile)
                    jenkinsfile.execute(params)
                }
            }
        }
    }
}
