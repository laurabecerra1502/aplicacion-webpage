pipeline {
    agent any

    stages {
        stage('Call another Jenkinsfile') {
            steps {
                script {
                    def params = [
                        string(branch: 'main', value: 'value1'),
                    ]

                    def jenkinsfile = load 'https://github.com/laurabecerra1502/devops.git'
                    scriptPath('jenkinsfile')
                    jenkinsfile.execute(params)
                }
            }
        }
    }
}
